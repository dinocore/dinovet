!!! 5
%html{html_attrs} 
 
  %head 
    %title Dinovet &rsaquo;
    = stylesheet_link_tag :all
    = javascript_include_tag :defaults
    = javascript_include_tag 'jquery-ui-1.8.5.custom.min.js'
    -if Rails.env == 'test' or Rails.env == 'cucumber' or Rails.env =='culerity'
      = javascript_include_tag 'culerity'
    = csrf_meta_tag
    :javascript
      var ROOT_PATH = '#{root_path}';

  %body 
 
    #header 
      %ul 
        %li 
          %a{:href => '#'} Reports 
        %li 
          %a{:href => '#'} Schedule 
        %li
          %a{:href => '#'} Clients
        %li
          %a{:href => '#'} Configure

  = yield

  != error_message
  != notice_message
